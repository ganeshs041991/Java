@Library('jenkins_shared_lib') _

pipeline{

     agent any


      stages{

                   stage( 'Git Checkout'){
               
                        steps{
                        gitCheckout(
                                branch: "master",
                                url: "https://github.com/ganeshs041991/Java.git"
                 )    
                 } 
           }

                   stage( 'Unit Test Maven'){
               
                        steps{
                           script{

                         mvnTest()
                 )    
                 } 
           }



 }

}


     
                         
